        <activity android:name="com.ogau.socialshare.SocialShareTestActivity" />
        <!-- MOB分享 -->
        <activity android:name="com.ogau.socialshare.mobshare.MobShareActivity" />
        <!-- ShareSDK的activity -->
        <activity
